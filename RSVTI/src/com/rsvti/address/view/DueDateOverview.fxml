<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rsvti.address.view.DueDateOverviewController">
   <children>
      <Label layoutX="65.0" layoutY="40.0" text="Privire generală a datelor scadente">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <GridPane layoutX="65.0" layoutY="101.0" prefHeight="47.0" prefWidth="700.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="186.0" minWidth="10.0" prefWidth="186.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="135.0" minWidth="10.0" prefWidth="16.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="238.0" minWidth="10.0" prefWidth="116.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="242.0" minWidth="10.0" prefWidth="242.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="De la data">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <DatePicker fx:id="dateFrom" GridPane.columnIndex="1" />
            <DatePicker fx:id="dateTo" GridPane.columnIndex="4" />
            <Label text="Până la data" GridPane.columnIndex="3">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </GridPane>
      <TableView fx:id="rigTable" layoutX="65.0" layoutY="196.0" prefHeight="544.0" prefWidth="447.0">
        <columns>
            <TableColumn fx:id="rigColumn" prefWidth="75.0" text="Utilaj" />
          <TableColumn fx:id="firmNameColumn" prefWidth="75.0" text="Firma" />
            <TableColumn fx:id="rigDueDateColumn" prefWidth="75.0" text="Data scadenței" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TableView fx:id="employeeTable" layoutX="535.0" layoutY="196.0" prefHeight="544.0" prefWidth="619.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn prefWidth="75.0" text="Angajat">
               <columns>
                  <TableColumn fx:id="employeeLastNameColumn" prefWidth="75.0" text="Nume" />
                  <TableColumn fx:id="employeeFirstNameColumn" prefWidth="75.0" text="Prenume" />
               </columns></TableColumn>
            <TableColumn fx:id="employeeRigColumn" prefWidth="75.0" text="Utilaj" />
            <TableColumn fx:id="employeeFirmNameColumn" prefWidth="75.0" text="Firma" />
            <TableColumn fx:id="employeeDueDateColumn" prefWidth="75.0" text="Data scadenței" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
